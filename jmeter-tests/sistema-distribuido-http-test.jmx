          <stringProp name="HTTPSampler.response_timeout">15000</stringProp>
          <stringProp name="HTTPSampler.postBodyRaw">true</stringProp>
          <elementProp name="HTTPsampler.postBodyRaw" elementType="Arguments">
            <collectionProp name="Arguments.arguments">
              <elementProp name="" elementType="HTTPArgument">
                <boolProp name="HTTPArgument.always_encode">false</boolProp>
                <stringProp name="Argument.value">{"id":"DATA-REQ-${__threadNum}-${__time}","type":"DATA_REQUEST","timestamp":"${__time(yyyy-MM-dd'T'HH:mm:ss.SSS)}","data":{"query":"performance_test","requestedBy":"CLIENT-${__threadNum}","requestId":"${__UUID}"}}</stringProp>

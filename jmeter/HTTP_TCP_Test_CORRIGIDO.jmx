  "messageType": "SENSOR_REGISTER",
  "sensorType": "${__Random(0,4,)}",
  "location": "HTTP_LOCATION_${__threadNum}",
  "timestamp": ${__time()},
  "versionVector": {
    "HTTP_SENSOR_${__threadNum}_${__counter(FALSE,)}": 1
